<toolset>
    <pathSet base="anaconda3/bin">
        <toolPath tool="dummy" path="python3.5"/>
    </pathSet>

    <pathSet base="$base">
        <pathVar name="dummy.script" path="dummy.py"/>
    </pathSet>

    <tool name="dummy">
        <execution env="binary" argConvention="adhoc">
            <arg pathRef="dummy.script"/>
        </execution>
    </tool>


</toolset>
